/*支付结果*/
.d_app {width: 100%;height: 5.1rem;padding: 0.5rem 10.5rem 0.5rem 7rem;box-sizing: border-box;background: rgba(0,0,0,0.8);position: relative;font-size: 1.2rem;position: fixed;bottom: 0;}
.d_app_close {width: 1.5rem;height: 1.5rem;position: absolute;left: 0;top: 0;background: url(../img/w_img/app-close.png) no-repeat left center;background-size: 15px;}
.d_app_logo {width: 4.4rem;height: 4.4rem;background: url(../img/w_img/app-logo.png) no-repeat left center;background-size: 44px;position: absolute;left: 1.5rem;top: 0.3rem;}
.d_app p {width: 100%;padding-top: 0.3rem;display: -webkit-box;-webkit-box-pack: center;-webkit-box-align: center;font-size: 1.4rem;color: #fff;}
.d_open_app {width: 9rem;height: 3.3rem;line-height: 3.3rem;color: #fff;text-align: center;border-radius: 5px;background: #c60d47;position: absolute;right: 0.5rem;top: 1.1rem;}
.d_nodata_img{width: 10.4rem;height: 5.1rem;margin: 2.4rem auto 0.6rem;}
.d_nodata_img img{ width: 100%;height: auto;}
.d_love_goods_tit { margin: 1.4rem 0rem;text-align: center;font-size: 1.5rem;line-height: 1.8rem;color: #666;position: relative;}
.d_love_goods_tit span {display: inline-block;position: relative; z-index: 2;width: 20rem;height: 1.8rem;background: #f5f5f5;}
.d_love_goods_tit:after {content: "";height: 1px;width: 100%;background: #e5e5e5;position: absolute;left: 0;top: 50%;z-index: 1;}
.d_zh_input{width: 100%;height: 4.5rem;line-height: 4.5rem;text-indent: 1.4rem;border:none;margin-top: 5px;font-size: 12px;color: #999;}
.d_save_btn{display:block;width: 14.5rem;height: 3.3rem;line-height: 3.3rem;color: #fff;text-align: center;border-radius: 5px;background: #dd0f50;margin:2rem auto 4rem;}
.d_yx{color:#999;font-size: 1rem;line-height: 2rem;}
/*支付结果-支付失败*/
.d_pay_fail{width: 100%; }
.d_pay_fail_img{width: 6.85rem;height: 6.45rem;margin: 2.4rem auto 0.5rem;}
.d_pay_fail_img img{ width: 100%;height: auto;}
.d_div_pay{ display:-webkit-box;-webkit-box-pack: center;-webkit-box-align: center;}
.d_div_pay p{line-height: 2.4rem;padding: 1.4rem 0rem;font-size: 15px;color:#333;}
.d_pay_fail_p{padding: 1.4rem 6.2rem;font-size: 15px;color:#333;line-height: 20px;text-align: center;}
.d_change_pay{display:block;margin:1.5rem auto;width: 10.45rem;height: 3.3rem;border:1px solid #c60d48;color:#c60d48;font-size: 15px;background: #fff;border-radius: 0.5rem;text-align: center;line-height: 3.5rem;}
.d_nodata2 {width: 100%;padding: 0 0 1rem;overflow: hidden;}
.d_txt_cen {display: -webkit-box;-webkit-box-pack: center; -webkit-box-align: center;}
.d_txt_cen div {-webkit-box-pack: center;-webkit-box-align: center;}
.d_nodata2 h2 {line-height: 3rem;text-align: center;font-size: 1.8rem;font-weight: normal;}
.d_nodata2 p {width: 100%;font-size: 1.5rem;line-height: 1.8rem;padding: .5rem 0 0;color: #333;display: block;}

/*特价活动列表页*/
.d_sale_header {background: #fff;color: #333;font-size: 0.18rem;height: 0.45rem;line-height: 0.45rem;text-align: center;position: relative;overflow: hidden;}
.d_jt_left::after{content: "";width: 0.21rem;height: 0.21rem;background: url(../img/d_btn_3.png);background-size: 100%;position: absolute;left: 0.1rem;top: 0.12rem;}
.d_sale_img1{  display: block;width: 100%;overflow: hidden;}
.d_sale_img1 img{  width: 100%;display: block;height: auto;}
.d_sale_det{width: 100%;height: 0.275rem;line-height: 0.275rem;position: relative;font-size: 0.12rem;overflow: hidden;background: #fff;}
.d_sale_det_bg{float:left;background: url(../img/dxx_img/d_zk_bg.png)no-repeat;background-size: 100%;width: 1.01rem;height: 0.275rem;color:#fff;font-size: 0.12rem;position: absolute;top: 0;left: 0;text-indent: 0.1rem;}
.d_count_down{color:#dc0f50;font-size: 0.12rem;}
.d_sale_goods{background: #f5f5f5;height: auto;padding:0.1rem;overflow: hidden;}
.d_sale_goods li{width: 50%;margin-bottom: 0.1rem;box-sizing: border-box;padding: 0 0.05rem;}
.d_sale_goods li a{display: block;color: #333;background: #fff;}
.d_sale_li_top{ width: 100%;height: auto;overflow: hidden;position: relative;}
.d_sale_li_top span{display: block;position: absolute;top: 50%;left: 50%;width: 0.63rem;height: 0.63rem;margin: -0.3rem 0 0 -0.3rem;z-index: 9;background: url(../img/dxx_img/ico-sx.png) no-repeat 0 0;background-size: 100% 100%;}
.d_sale_intr{position: relative;width: 100%;padding:0.08rem 0.1rem;box-sizing: border-box;-webkit-box-sizing: border-box;}
.d_sale_intr h3{height:0.32rem;font-size: 0.12rem;color:#222222;line-height: .16rem;margin: .05em 0 .04rem;font-weight: normal;overflow: hidden;}
.d_sale_price{font-size: 0.16rem;color: #dc0f50;padding: .06rem 0 .01rem;font-weight: normal;overflow: hidden;position: relative;}
.d_sale_price b{font-size: .1rem;font-weight: normal;}
.d_sale_price_span{  color: #999;font-size: 0.12rem;font-weight: normal;padding: 0 0 0 .08rem;text-decoration: line-through;}
.d_fix_icod{ position: absolute;right: 0;bottom: .05rem;overflow: hidden;color: #fff;background: #dc6c8f;border-radius: 2px;-webkit-border-radius: 2px;height: .16rem;line-height: .16rem;width: .32rem;padding: 0 .01rem;margin: 0 .05rem 0 0;font-size: .1rem;text-align: center;display: inline-block;}
/*今日新品 海外购*/
.d_jrxp_content{margin:0 auto;}
.d_today_new{margin: 0 0 2% 0;width: 100%;float: left;min-height: 100px;background: #FFF;}
